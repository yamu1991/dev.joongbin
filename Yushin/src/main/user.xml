SELECT USER_ID
     , USER_NAME
     , USER_AGE
     , REG_DT
  FROM TB_USER_BASE
 WHERE 1=1
   AND USER_ID = :USER_ID
;

INSERT INTO TB_USER_BASE
(USER_ID, USER_NAME, USER_AGE, REG_DT)
VALUES
(:USER_ID, :USER_NAME, :USER_AGE, SYSDATE)
;


SELECT USER_ID
     , ACCT_NO
     , LAST_SEQ
     , REG_DT
     , UPD_DT
  FROM TB_ACCT_BASE
 WHERE 1=1
   AND USER_ID = :USER_ID
;

INSERT INTO TB_ACCT_BASE
(USER_ID, ACCT_NO, REG_DT, UPD_DT)
VALUES
(:USER_ID, :ACCT_NO, SYSDATE, SYSDATE)
;



